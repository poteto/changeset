{
  "name": "validated-proxy",
  "version": "0.2.0",
  "description": "Validated proxy",
  "main": "dist/validated-proxy.umd.js",
  "module": "dist/validated-proxy.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --module commonjs --outDir dist/es && rollup -c rollup.config.ts",
    "build:docs": "typedoc --includeDeclarations --excludeExternals --out docs --target es6 --mode file src ",
    "deploy:docs": "yarn run build:docs && gh-pages -d docs",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "prepublishOnly": "yarn run build",
    "postpublish": "yarn run deploy:docs",
    "release": "release-it",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:prod": "yarn run lint && yarn run test --coverage --no-cache",
    "test:watch": "jest --watch"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "author": "Lauren Tan <arr@sugarpirate.com> (http://www.sugarpirate.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/poteto/validated-proxy"
  },
  "keywords": [
    "buffer",
    "validated proxy",
    "changeset",
    "typescript"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "@types/sinon": "^7.0.5",
    "coveralls": "^3.0.2",
    "gh-pages": "^2.0.1",
    "jasmine-check": "^1.0.0-rc.0",
    "jest": "^23.6.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.16.3",
    "release-it": "^10.0.7",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "sinon": "^7.2.3",
    "testcheck": "^1.0.0-rc.2",
    "ts-jest": "^23.10.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.1"
  }
}
