{
  "name": "validated-proxy",
  "version": "0.0.0",
  "description": "Validated proxy",
  "main": "dist/validated-proxy.umd.js",
  "module": "dist/validated-proxy.es5.js",
  "typings": "dist/types/validated-proxy.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc && tsc --module commonjs --outDir dist/es && rollup -c rollup.config.ts && rimraf compiled",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "mapCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "author": "Lauren Tan <arr@sugarpirate.com> (http://www.sugarpirate.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/poteto/validated-proxy"
  },
  "keywords": [
    "buffer",
    "validated proxy",
    "changeset",
    "typescript"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.58",
    "@types/sinon": "^4.1.2",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "jsverify": "^0.8.3",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "sinon": "^4.1.3",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2"
  }
}
